.divider {
    margin: 4px 0;
}

.menu {
    margin: 0;
}

.plate-barcode-cell, .plate-barcode-cell > * {
    border-radius: unset !important;
    box-shadow: none;
    height: 33px;
    margin: 0;
    outline: none;
    overflow: hidden;
    width: 100%;
}

.see-metadata-button {
    font-size: 11px;
    padding: 0 6px;
}

.modal-description {
    font-size: 12px;
    color: rgba(0, 0, 0, 0.65);
}

.loading-spinner {
    text-align: center;
}
